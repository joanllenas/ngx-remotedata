{
    "name": "ngx-remotedata",
    "version": "8.0.0",
    "description": "RemoteData: Slaying a UI Antipattern with Angular",
    "repository": {
        "type": "git",
        "url": "https://github.com/joanllenas/ngx-remotedata"
    },
    "keywords": [
        "angular",
        "ngx",
        "angular2",
        "ng2",
        "pipes",
        "remote-data",
        "elm"
    ],
    "author": {
        "name": "Joan Llenas Mas√≥",
        "email": "joan.llenas.maso@gmail.com",
        "url": "http://joanllenas.com"
    },
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/joanllenas/ngx-remotedata/issues"
    },
    "homepage": "https://github.com/joanllenas/ngx-remotedata#readme",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build ngx-remotedata",
        "watch": "ng build --watch --configuration development",
        "test": "ng test ngx-remotedata --watch=false",
        "sync": "cp ./README.md ./dist/ngx-remotedata && sync-json -v --property version --source package.json dist/ngx-remotedata/package.json",
        "prerelease": "npm run test && npm run build && npm run sync",
        "release": "npm publish 'dist/ngx-remotedata/'",
        "beta": "npm run prerelease && npm publish 'dist/ngx-remotedata/' --tag beta"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.0.0",
        "@angular/common": "^16.0.0",
        "@angular/compiler": "^16.0.0",
        "@angular/core": "^16.0.0",
        "@angular/forms": "^16.0.0",
        "@angular/platform-browser": "^16.0.0",
        "@angular/platform-browser-dynamic": "^16.0.0",
        "@angular/router": "^16.0.0",
        "@ngrx/effects": "^16.0.0",
        "@ngrx/store": "^16.0.0",
        "@ngrx/store-devtools": "^16.0.0",
        "ngrx-store-localstorage": "^16.0.0",
        "rxjs": "^7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "^0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.0.0",
        "@angular/cli": "^16.0.0",
        "@angular/compiler-cli": "^16.0.0",
        "@types/jasmine": "^3.10.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "^4.0.0",
        "karma": "^6.3.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.1.0",
        "karma-jasmine": "^4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "ng-packagr": "^16.0.0",
        "prettier": "^2.5.1",
        "sync-json": "^1.0.2",
        "typescript": "^5.0.0"
    }
}
